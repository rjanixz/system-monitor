<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width initial-scale=1.0">
    <title>Sistemas Operativos 1</title>
    <!-- GLOBAL MAINLY STYLES-->
    <link href="vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="vendors/line-awesome/css/line-awesome.min.css" rel="stylesheet" />
    <link href="vendors/themify-icons/css/themify-icons.css" rel="stylesheet" />
    <link href="vendors/animate.css/animate.min.css" rel="stylesheet" />
    <!-- PLUGINS STYLES-->
    <!-- THEME STYLES-->
    <link href="css/main.min.css" rel="stylesheet" />
    <!-- PAGE LEVEL STYLES-->
</head>

<body>
    <div class="page-wrapper">
        <!-- START HEADER-->
        <header class="header">
            <!-- START TOP-LEFT TOOLBAR-->
            <ul class="nav navbar-toolbar">
                <li>
                    <a class="nav-link sidebar-toggler js-sidebar-toggler" href="javascript:;">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </a>
                </li>
            </ul>
            <!-- END TOP-LEFT TOOLBAR-->
            <!--LOGO-->
            <a class="page-brand" href="index.html">SO</a>
            <!-- START TOP-RIGHT TOOLBAR-->
            <ul class="nav navbar-toolbar">
                <li class="dropdown dropdown-user">
                    <a class="nav-link dropdown-toggle link" data-toggle="dropdown">
                        <span>Admin</span>
                    </a>
                </li>
            </ul>
            <!-- END TOP-RIGHT TOOLBAR-->
        </header>
        <!-- END HEADER-->
        <!-- START SIDEBAR-->
        <nav class="page-sidebar">
            <ul class="side-menu metismenu scroller">
                <li class="active">
                    <a href="javascript:;"><i class="sidebar-item-icon ti-home"></i>
                        <span class="nav-label">Dashboards</span><i class="fa fa-angle-left arrow"></i></a>
                    <ul class="nav-2-level collapse in">
                        <li>
                            <a class="active" href="/">Procesos</a>
                        </li>
                        <li>
                            <a href="/ram">RAM</a>
                        </li>
                        <li>
                            <a href="/cpu">CPU</a>
                        </li>
                    </ul>
                </li>
            </ul>
        </nav>
        <!-- END SIDEBAR-->
        <div class="content-wrapper">
            <!-- START PAGE CONTENT-->
            <div class="page-content fade-in-up">
                <div class="row mb-4">
                    <div class="col-lg-3 col-md-6">
                        <div class="card mb-4">
                            <div class="card-body flexbox-b">
                                <div class="easypie mr-4" data-percent="1" data-bar-color="#5c6bc0" data-size="80" data-line-width="8">
                                    <span class="easypie-data text-primary" style="font-size:32px;"><i class="la la-play"></i></span>
                                </div>
                                <div>
                                    <h3 class="font-strong text-primary">{{totalProcessesRunning}}</h3>
                                    <div class="text-muted">En ejecución (R)</div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="card mb-4">
                            <div class="card-body flexbox-b">
                                <div class="easypie mr-4" data-percent="80" data-bar-color="#ff4081" data-size="80" data-line-width="8">
                                    <span class="easypie-data font-26 text-pink"><i class="la la-pause"></i></span>
                                </div>
                                <div>
                                    <h3 class="font-strong text-pink">{{totalProcessesSuspended}}</h3>
                                    <div class="text-muted">Suspendidos (S)</div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="card mb-4">
                            <div class="card-body flexbox-b">
                                <div class="easypie mr-4" data-percent="1" data-bar-color="#18c5a9" data-size="80" data-line-width="8">
                                    <span class="easypie-data text-success" style="font-size:32px;"><i class="la la-stop"></i></span>
                                </div>
                                <div>
                                    <h3 class="font-strong text-success">{{totalProcessesStopped}}</h3>
                                    <div class="text-muted">Detenidos (T)</div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="card mb-4">
                            <div class="card-body flexbox-b">
                                <div class="easypie mr-4" data-percent="1" data-bar-color="#f39c12" data-size="80" data-line-width="8">
                                    <span class="easypie-data text-warning" style="font-size:32px;"><i class="la la-times"></i></span>
                                </div>
                                <div>
                                    <h3 class="font-strong text-warning">{{totalProcessesZombie}}</h3>
                                    <div class="text-muted">Zombies (Z)</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                
                <div class="row">
                    <div class="col-lg-12">
                        <div class="ibox ibox-fullheight">
                            <div class="ibox-head">
                                <div class="ibox-title">
                                    <span class="btn-icon-only btn-circle bg-primary-50 text-primary mr-3"><i class="la la-toggle-right"></i></span>Procesos</div>
                            </div>
                            <div class="ibox-body">
                                <div class="flexbox mb-4">
                                    <div class="flexbox">
                                        <span class="flexbox mr-3">
                                            <span class="mr-2 text-muted">Total</span>
                                            <span class="h3 mb-0 text-primary font-strong">{{totalProcesses}}</span>
                                        </span>
                                    </div>
                                </div>
                                <div class="ibox-fullwidth-block">
                                    <table class="table">
                                        <thead class="thead-default thead-lg">
                                            <tr>
                                                <th class="pl-4">Nombre</th>
                                                <th>PID</th>
                                                <th>Usuario</th>
                                                <th class="pr-4">% Memoria</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#each list}}
                                            <tr>
                                                <td class="pl-4">
                                                    <div class="flexbox-b">
                                                        <span class="btn-icon-only btn-{{this.cssclass}} font-20 mr-3">{{this.state}}</span>
                                                        <div>
                                                            <h6 class="mb-1">{{this.name}}</h6>
                                                        </div>
                                                    </div>
                                                </td>
                                                <td>
                                                    <h4 class="font-strong text-{{this.cssclass}} mb-0">{{this.pid}}</h4>
                                                </td>
                                                <td>
                                                    <h4 class="font-strong text-light mb-0">{{this.userName}}</h4>
                                                </td>
                                                <td class="pr-4">
                                                    <div class="easypie" data-percent="{{this.memP}}" data-bar-color="{{this.color}}" data-size="56" data-line-width="3">
                                                        <span class="easypie-data h5 font-strong">{{this.memP}}%</span>
                                                    </div>
                                                </td>
                                            </tr>
                                            {{/each}}
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- END PAGE CONTENT-->
            <footer class="page-footer">
                <div class="font-13">2019 <b>Vacaciones Junio</b> - Sistemas Operativos 1, Práctica 1</div>

                <div class="to-top"><i class="fa fa-angle-double-up"></i></div>
            </footer>
        </div>
    </div>

    <!-- CORE PLUGINS-->
    <script src="vendors/jquery/dist/jquery.min.js"></script>
    <script src="vendors/popper.js/dist/umd/popper.min.js"></script>
    <script src="vendors/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="vendors/metisMenu/dist/metisMenu.min.js"></script>
    <script src="vendors/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <script src="vendors/jquery-validation/dist/jquery.validate.min.js"></script>
    <!-- PAGE LEVEL PLUGINS-->
    <script src="vendors/chart.js/dist/Chart.min.js"></script>
    <script src="vendors/jquery.easy-pie-chart/dist/jquery.easypiechart.min.js"></script>
    <!-- CORE SCRIPTS-->
    <script src="js/app.min.js"></script>
    <!-- PAGE LEVEL SCRIPTS-->
    <script src="js/scripts/dashboard_visitors.js"></script>
</body>